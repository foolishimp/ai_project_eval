{
  "name": "ai_project_eval-test-dashboard",
  "version": "1.0.0",
  "description": "Test dashboard for ai_project_eval project (from test-dd-dashboard)",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "discover": "node scripts/discover-tests.js",
    "test": "npm run test:self && npm run test:e2e",
    "test:self": "echo 'Open http://localhost:8085/tests/dashboard-self-test.html to run self-tests'",
    "test:e2e": "node tests/dashboard-e2e.test.cjs",
    "test:claude": "node tests/claude-init-check.js",
    "check": "npm run test:claude"
  },
  "keywords": [
    "test",
    "dashboard",
    "testing",
    "playwright",
    "test-runner",
    "test-discovery",
    "multi-language"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "playwright": "^1.54.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dashboard.html",
    "server.js",
    "scripts/",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/foolishimp/test_dd_dashboard.git"
  }
}